{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 100,
        "ignoreUrls": true
      }
    ],
    "comma-dangle": 0,
    "eol-last": [
      "error",
      "always"
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true
  }
}
